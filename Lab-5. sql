​         ​create​ ​table​ ​Students 
 ​( 
 ​        StuID ​int​, 
 ​        FirstNmae ​VARCHAR​(​25​), 
 ​        LastName ​VARCHAR​(​25​), 
 ​        Website ​VARCHAR​(​50​), 
 ​        City ​VARCHAR​(​25​), 
 ​        Division ​VARCHAR​(​20​) 
 ​) 
  
 ​SELECT​ ​*​FROM​ Students 
  
